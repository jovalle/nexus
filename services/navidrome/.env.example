# Navidrome Environment Variables
# Copy this file to .env and fill in your values

# =====================
# API Credentials (for homepage widget)
# =====================

# Username for Subsonic API access
NAVIDROME_API_USER=

# Subsonic API token (MD5 hash of password+salt)
NAVIDROME_API_TOKEN=

# Salt used for token generation
NAVIDROME_API_SALT=
