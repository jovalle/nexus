# Gluetun VPN Environment Variables
# Copy this file to .env and fill in your values

# =====================
# API Configuration
# =====================

# API key for Gluetun control server (REQUIRED)
GLUETUN_API_KEY=

# =====================
# OpenVPN Configuration
# =====================

# OpenVPN provider name
OPENVPN_PROVIDER=

# OpenVPN credentials
OPENVPN_USER=
OPENVPN_PASSWORD=

# OpenVPN server list (comma-separated)
OPENVPN_SERVERS=

# VPN server region
SERVER_REGIONS=

# =====================
# WireGuard Configuration
# =====================

# WireGuard interface address (REQUIRED)
# Example: 10.0.0.2/32
WIREGUARD_ADDRESSES=

# VPN server endpoint IP (REQUIRED)
WIREGUARD_ENDPOINT_IP=

# VPN server endpoint port
WIREGUARD_ENDPOINT_PORT=51820

# Your WireGuard private key (REQUIRED)
WIREGUARD_PRIVATE_KEY=

# VPN server's public key (REQUIRED)
WIREGUARD_PUBLIC_KEY=
