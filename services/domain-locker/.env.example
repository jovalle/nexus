# Domain Locker Environment Variables
# Copy this file to .env and fill in your values

# PostgreSQL database connection settings
DOMAIN_LOCKER_DB_USER=domain_locker
DOMAIN_LOCKER_DB_NAME=domain_locker
DOMAIN_LOCKER_DB_PASSWORD=

# OIDC client secret hash for Authelia integration (REQUIRED)
# Generate with: authelia crypto hash generate pbkdf2
DOMAIN_WATCHDOG_SECRET=
