# AdGuard Home Environment Variables
# Copy this file to .env and fill in your values

# Admin credentials for AdGuard Home web interface
ADGUARD_ADMIN_USERNAME=
ADGUARD_ADMIN_PASSWORD=

# Pre-hashed admin password for AdGuard Home (REQUIRED)
# Generate hash: htpasswd -nbB admin 'yourpassword' | cut -d: -f2
ADGUARD_ADMIN_PASSWORD_HASHED=

# Static IP address for AdGuard container on the nexus network
ADGUARD_IP=172.52.0.100
